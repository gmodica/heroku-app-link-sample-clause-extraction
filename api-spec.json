{
    "openapi": "3.0.1",
    "info": {
        "title": "api | v1",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://localhost:7177/"
        }
    ],
    "paths": {
        "/extract-async": {
            "post": {
                "summary": "Extracts clauses asynchronously from a given URL",
				"description": "Submits a job to execute asynchronously. Downloads the ZIP at the provided URL, extracts files in-memory and parses each as XML. Then clauses are create in Salesforce via Bulk API",
				"operationId": "ExtractFilesAsync",
				"x-sfdc": {
					"heroku": {
						"authorization": {
							"connectedApp": "ClausesExtractionAppLinkConnectedApp",
							"permissionSet": "ClausesExtraction"
						}
					}
                },
                "tags": [
                    "ExtractJob"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        },
                        "text/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        },
                        "application/*+json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "202": {
                        "description": "Accepted",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ExtractJobResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/extract": {
            "post": {
                "summary": "Extracts clauses from a given URL",
				"description": "Downloads the ZIP at the provided URL, extracts files in-memory and parses each as XML to return parsed objects",
				"operationId": "ExtractFiles",
				"x-sfdc": {
					"heroku": {
						"authorization": {
							"connectedApp": "ClausesExtractionAppLinkConnectedApp",
							"permissionSet": "ClausesExtraction"
						}
					}
                },
                "tags": [
                    "Extract"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        },
                        "text/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        },
                        "application/*+json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExtractRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ExtractResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "504": {
                        "description": "Gateway Timeout",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ErrorFile": {
                "required": [
                    "fileName",
                    "error"
                ],
                "type": "object",
                "properties": {
                    "fileName": {
                        "type": "string"
                    },
                    "error": {
                        "type": "string"
                    }
                }
            },
            "ErrorResponse": {
                "required": [
                    "error"
                ],
                "type": "object",
                "properties": {
                    "error": {
                        "type": "string"
                    },
                    "details": {
                        "type": "string",
                        "default": null,
                        "nullable": true
                    }
                }
            },
            "ExtractJobResponse": {
                "required": [
                    "url",
                    "jobId"
                ],
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    },
                    "jobId": {
                        "type": "string"
                    }
                }
            },
            "ExtractRequest": {
                "required": [
                    "url"
                ],
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    }
                }
            },
            "ExtractResponse": {
                "required": [
                    "url",
                    "files"
                ],
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    },
                    "files": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ParsedFile"
                        }
                    },
                    "errors": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ErrorFile"
                        },
                        "default": null,
                        "nullable": true
                    }
                }
            },
            "ParsedFile": {
                "required": [
                    "fileName",
                    "id",
                    "name",
                    "number",
                    "body"
                ],
                "type": "object",
                "properties": {
                    "fileName": {
                        "type": "string"
                    },
                    "id": {
                        "type": "string",
                        "nullable": true
                    },
                    "name": {
                        "type": "string",
                        "nullable": true
                    },
                    "number": {
                        "type": "string",
                        "nullable": true
                    },
                    "body": {
                        "type": "string",
                        "nullable": true
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Extract"
        }
    ]
}